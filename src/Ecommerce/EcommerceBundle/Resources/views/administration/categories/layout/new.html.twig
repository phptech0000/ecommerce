{% extends 'layout/layoutAdmin.html.twig' %}

{% block body %}
    <h1>Création d'une nouvelle catégorie</h1>

    <div class="row">
        <div class="span8">
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <input class="btn btn-primary" type="submit" value="Créer" />
            {{ form_end(form) }}
        </div>
    </div>


    <div class="row">
        <div class="span8">
            <ul style="list-style: none">
                <li>
                    <a class="btn btn-default" href="{{ path('adminCategories_index') }}">Retour</a>
                </li>
            </ul>
        </div>
    </div>
{% endblock %}
